{
  "name": "claude-manager",
  "version": "1.7.0",
  "description": "Terminal app for managing Claude Code settings, profiles, MCP servers, and skills",
  "type": "module",
  "bin": {
    "cm": "./dist/cli.js",
    "claude-manager": "./dist/cli.js"
  },
  "scripts": {
    "build": "esbuild src/cli.js --platform=node --format=esm --loader:.js=jsx --bundle --outfile=dist/cli.js --packages=external && echo '#!/usr/bin/env node' | cat - dist/cli.js > dist/tmp && mv dist/tmp dist/cli.js && chmod +x dist/cli.js",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "claude",
    "claude-code",
    "anthropic",
    "ai",
    "cli",
    "mcp",
    "profile-manager"
  ],
  "author": "Faisal Nazir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/faisalnazir/claude-manager.git"
  },
  "homepage": "https://github.com/faisalnazir/claude-manager#readme",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "fuse.js": "^7.1.0",
    "ink": "^5.1.0",
    "ink-select-input": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "vitest": "^2.1.8"
  }
}
